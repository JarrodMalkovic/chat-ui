{
    "builds": [
      {
        "buildCommand": "ADAPTER=vercel vite build",
        "src": "package.json",
        "use": "@vercel/node",
        "config": { "includeFiles": ["build-vercel/**"] },
        "outputDirectory": "build-vercel"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "build-vercel/index.js"
      }
    ]
  }
  